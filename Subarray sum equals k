class Solution { 
    public: int subarraySum(vector<int>& nums, int k) 
    { 
        unordered_map<int,int>mp; 
        int sum=0; 
        int ans=0; 
        mp[sum]=1;
        for(auto it:nums) 
        { 
            sum=sum+it; 
        int fin=sum-k; 
        if(mp.find(fin)!=mp.end()) 
        { 
            ans=ans+mp[fin]; 
        } 
          mp[sum]++;
        
         } 
         return ans; 
     } 
    };
